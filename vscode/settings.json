{
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontSize": 15,
  "editor.largeFileOptimizations": false,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "timeline.excludeSources": [],
  "terminal.integrated.tabs.enabled": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.statusBar.visible": true,

  "bashBeautify.tabSize": 2,

  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,

  "cSpell.language": "en,en-GB,en-US",
  "cSpell.userWords": [
    "Ankit",
    "BSDF",
    "Fibo",
    "Harshit",
    "IITB",
    "Misra",
    "OPENGL",
    "Outlab",
    "Outof",
    "RGBA",
    "Richeek",
    "Signup",
    "Struct",
    "Thue",
    "Vimba",
    "algo",
    "amssymb",
    "amsthm",
    "arange",
    "astropy",
    "atmost",
    "changepwd",
    "cout",
    "currentuserid",
    "devansh",
    "downlevel",
    "dtype",
    "ecsv",
    "eevee",
    "enumitem",
    "esint",
    "favicon",
    "fname",
    "gmail",
    "groupby",
    "imgs",
    "imshow",
    "inorder",
    "itemsep",
    "itertools",
    "matplotlib",
    "mollweide",
    "moodle",
    "mplot",
    "ncols",
    "normalise",
    "nrows",
    "numpy",
    "organising",
    "orgs",
    "otypes",
    "pastchecks",
    "plagsample",
    "plagzip",
    "plotly",
    "plotlyjs",
    "profileid",
    "psycopg",
    "pygame",
    "pyplot",
    "robosub",
    "roscore",
    "rospkg",
    "rospy",
    "scipy",
    "seaborn",
    "sectionfont",
    "serializers",
    "sinc",
    "smtplib",
    "sqlite",
    "textcolor",
    "tokenkey",
    "tokenmanager",
    "unplag",
    "upassword",
    "userid",
    "vectorize",
    "visualise",
    "xlabel",
    "xlim",
    "xlrd",
    "xmax",
    "xmin",
    "xticks",
    "ylabel",
    "ylim",
    "yticklabels",
    "yticks",
    "zlabel",
    "zlim",
    "zorder"
  ],

  "clang-format.executable": "clang-format-10",

  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },

  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "html.format.endWithNewline": true,

  "git.autofetch": true,
  "git.confirmSync": false,
  "gitlens.defaultTimeFormat": "HH:mm",
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,

  "[java]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "java.autobuild.enabled": false,

  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.bibtex-format.tab": "4 spaces",
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.synctex(busy)",
    "*.synctex",
    "*.synctex.gz(busy)",
    "*.synctex.gz",
    "*.nav"
  ],

  "prolog.executablePath": "/usr/bin/prolog",
  "[prolog]": {
    "editor.defaultFormatter": "arthurwang.vsc-prolog"
  },

  "python.venvPath": "~/virtualenvs",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--max-line-length=80", "--ignore=F405, F403"],

  "python.formatting.autopep8Args": [
    "--max-line-length=80",
    "--ignore=F405, F403"
  ],
  "python.languageServer": "Microsoft",
  "jupyter.askForKernelRestart": false,

  "[scss]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },

  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },

  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },

  "vhdl.formatter.case.keyword": "LowerCase",
  "vhdl.formatter.case.typename": "LowerCase",
  "vhdl.formatter.insertFinalNewline": true,
  "vhdl.formatter.align.procedure": true,
  "vhdl.formatter.align.port": true,
  "vhdl.formatter.align.function": true
}

